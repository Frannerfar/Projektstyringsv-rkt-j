<Window x:Class="Projektstyring.ModalAddStage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projektstyring"
        mc:Ignorable="d"
        Title="ModalAddStage" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        
        <TextBox x:Name="StageTitle" Grid.Row="0" Width="200" Height="50"/>
        <Button x:Name="AddStageButton" Grid.Row="1" Click="AddStageButton_Click" />
    </Grid>
</Window>
